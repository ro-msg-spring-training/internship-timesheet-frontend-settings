<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog contentWidth="30%" title="{i18n>psps}">
		<content>
		<Table mode="SingleSelectMaster" itemPress=".handleClose" noDataText="No PSPs" items="{path : 'users>/createdPspsData'}">
			<ColumnListItem>
				<cells>
					<ObjectIdentifier title="{users>name}"/>
				</cells>
			</ColumnListItem>
			<columns>
				<Column width="12em">
					<header>
						<Text text="{i18n>pspName}"/>
					</header>
				</Column>
			</columns>
		</Table>
        </content>
        <beginButton>
            <Button text="{i18n>cancelButton}" press=".onExit"/>
        </beginButton>
	</Dialog>
</core:FragmentDefinition>