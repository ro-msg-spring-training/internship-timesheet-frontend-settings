{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ui.demo.fiori2",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.demo.fiori2.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.demo.fiori2.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.demo.fiori2.view",
				"controlId": "layout",
				"controlAggregation": "beginColumnPages",
				"transition": "slide",
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "login",
				"target": "login"
			}, {
				"pattern": "master",
				"name": "master",
				"target": "master"
			}, {
				"pattern": "master/detail/{programId}",
				"name": "detail",
				"target": "detail"
			}, {
				"pattern": "wizard",
				"name": "wizard",
				"target": "wizard"
			}],
			"targets": {
				"login": {
					"viewName": "Login",
					"viewLevel": 1,
					"viewId": "login"
				},
				"master": {
					"viewName": "Master",
					"viewLevel": 1
				},
				"detail": {
					"viewName": "Detail",
					"viewLevel": 1
				},
				"wizard": {
					"viewName": "Wizard",
					"viewLevel": 1
				}
			}
		},
		"config": {
			"fullWidth": true
		},
		"contentDensities": {
			"compact": false,
			"cozy": false
		},
		"flexEnabled": true
	}
}